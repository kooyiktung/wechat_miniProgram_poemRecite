page{
  background-color: #F5F5F5
}

/* 定义.container设置容器样式 */
.container{
  padding: 10px 20px;
  align-content: center;
}

.title {
  width: 90%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #0094FF;
}

/* 定义.text-content设置文本样式 */
.text-content{
  width: 86%;
  padding: 10px;
  color: #0094FF;
  align-content: center;
  border-bottom: 1rpx solid #F5F5F5;
  background-color: #FFFFFF
}
